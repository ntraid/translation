{
    "name": "ntraid/translation",
    "description": "A Translation package for Laravel 9 with database and cache support. Fork of the package Waavi Translation.",
    "keywords": [
        "laravel-translator",
        "laravel",
        "translator",
        "translation",
        "localization"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2 || ^8.3 || ^8.4",
        "laravel/framework": "^12.0",
        "doctrine/dbal": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "orchestra/testbench": "^9.4",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Waavi\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waavi\\Translation\\Test\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
